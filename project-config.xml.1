<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project SYSTEM "../../project-config.dtd">
<project>
  <name>synchronized VCS</name>
  <parameters />
  <cleanup>
    <policy type="days" cleanup-level="EVERYTHING">
      <parameters>
        <param name="keepDays.count" value="2" />
      </parameters>
    </policy>
    <policy type="days" cleanup-level="HISTORY_ENTRY">
      <parameters>
        <param name="keepDays.count" value="2" />
      </parameters>
    </policy>
    <policy type="days" cleanup-level="ARTIFACTS">
      <parameters>
        <param name="artifactPatterns" value="ss" />
        <param name="keepDays.count" value="2" />
      </parameters>
    </policy>
    <options>
      <option name="preventDependenciesArtifactsFromCleanup" value="true" />
    </options>
  </cleanup>
</project>

